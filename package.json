{
  "name": "ts-s3-metrics",
  "version": "1.0.0",
  "description": "Lib that allows you to read or write metrics from s3",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "scripts": {
    "build": "npx tsc",
    "lint": "npx tslint src/*",
    "lint:fix": "npx tslint src/* --fix",
    "docs": "npx typedoc src",
    "test": "npx jest src",
    "test:it": "npx jest -c jest.it.config.js src",
    "test:coverage": "npx jest --coverage src && npm run test:badge",
    "test:badge": "npx lcov-badge-generator -o coverage/badge.svg coverage/lcov.info",
    "test:watch": "npx jest --watchAll src"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "keywords": [
    "s3",
    "AWS",
    "metrics"
  ],
  "sideEffects": false,
  "devDependencies": {
    "@types/jest": "^25.2.2",
    "@types/node": "^14.14.27",
    "jest": "^25",
    "ts-jest": "^25.5.1",
    "tslint": "^5",
    "tslint-config-airbnb": "^5.11.2",
    "typedoc": "^0.17.6",
    "typescript": "^3.9.2"
  },
  "author": "Johan Ablett-Karlsson",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.843.0",
    "builder-pattern": "^1.2.4"
  }
}
